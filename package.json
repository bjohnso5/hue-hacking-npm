{
  "name": "hue-hacking-node",
  "version": "1.6.0",
  "description": "Utility to control Philips Hue light bulbs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjohnso5/hue-hacking-npm"
  },
  "scripts": {
    "test": "nyc --reporter=lcov ava-ts --verbose && gulp",
    "clean": "npx rimraf dist coverage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npx rimraf dist && ava-ts && ngc && gulp"
  },
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [
    "Philips",
    "Hue"
  ],
  "author": "Bryan Johnson",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@babel/register": "^7.4.4",
    "@types/node": "^10.14.9",
    "ava": "^2.1.0",
    "ava-ts": "^0.25.2",
    "coveralls": "^3.0.4",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "moxios": "^0.4.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "ts-node": "^8.3.0",
    "typescript": "3.2.x",
    "zone.js": "^0.9.1"
  }
}
