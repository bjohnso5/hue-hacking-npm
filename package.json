{
  "name": "hue-hacking-node",
  "version": "1.2.0",
  "description": "Utility to control Philips Hue light bulbs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjohnso5/hue-hacking-npm"
  },
  "scripts": {
    "test": "ngc && nyc --reporter=lcov ava --verbose && gulp",
    "clean": "rm -rf dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "rm -rf dist && ngc && ava && gulp"
  },
  "ava": {
    "files": [
      "src/*.spec.js"
    ],
    "source": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "exclude": [
      "src/**.spec.*",
      "src/hue-test-constants.*"
    ]
  },
  "keywords": [
    "Philips",
    "Hue"
  ],
  "author": "Bryan Johnson",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2"
  },
  "devDependencies": {
    "@angular/animations": "^4.3.4",
    "@angular/common": "^4.3.4",
    "@angular/compiler": "^4.3.4",
    "@angular/compiler-cli": "^4.3.4",
    "@angular/core": "^4.3.4",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-server": "^4.3.4",
    "@types/node": "^7.0.39",
    "ava": "^0.19.1",
    "coveralls": "^2.13.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "moxios": "^0.4.0",
    "nyc": "^11.0.1",
    "rxjs": "^5.4.3",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.16"
  }
}
