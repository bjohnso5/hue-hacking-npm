{
  "name": "hue-hacking-node",
  "version": "1.1.8",
  "description": "Utility to control Philips Hue light bulbs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjohnso5/hue-hacking-npm"
  },
  "scripts": {
    "test": "ngc && nyc --reporter=lcov ava --verbose && gulp",
    "clean": "rm -rf dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "rm -rf dist && ngc && ava && gulp"
  },
  "ava": {
    "files": [
      "src/*.spec.js"
    ],
    "source": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "exclude": [
      "src/**.spec.*",
      "src/hue-test-constants.*"
    ]
  },
  "keywords": [
    "Philips",
    "Hue"
  ],
  "author": "Bryan Johnson",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3"
  },
  "devDependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/compiler-cli": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-server": "^2.4.10",
    "@types/node": "^7.0.0",
    "ava": "^0.17.0",
    "coveralls": "^2.11.15",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "moxios": "^0.3.0",
    "nyc": "^10.1.2",
    "rxjs": "^5.2.0",
    "typescript": "^2.2.1",
    "zone.js": "^0.8.5"
  }
}
