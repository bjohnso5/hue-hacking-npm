{
  "name": "hue-hacking-node",
  "version": "1.3.0",
  "description": "Utility to control Philips Hue light bulbs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjohnso5/hue-hacking-npm"
  },
  "scripts": {
    "test": "ngc && nyc --reporter=lcov ava --verbose && gulp",
    "clean": "rm -rf dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "node ./node_modules/rimraf/bin.js dist && ngc && ava && gulp"
  },
  "ava": {
    "files": [
      "src/*.spec.js"
    ],
    "sources": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "exclude": [
      "src/**.spec.*",
      "src/hue-test-constants.*"
    ]
  },
  "keywords": [
    "Philips",
    "Hue"
  ],
  "author": "Bryan Johnson",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/core": "^7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/platform-server": "^7.2.9",
    "@types/node": "^10.12.24",
    "ava": "^1.3.1",
    "coveralls": "^3.0.3",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "moxios": "^0.4.0",
    "nyc": "^14.1.1",
    "rxjs": "^6.4.0",
    "typescript": "3.2.x",
    "zone.js": "^0.8.29"
  }
}
